/* ICON and Version info for Simutrans-Experimental Windows */

#include "simversion.h"

100 ICON "simutrans.ico"
101 ICON "old.ico"


1 VERSIONINFO
FILEVERSION    RES_VERSION_NUMBER
PRODUCTVERSION RES_VERSION_NUMBER
{
 BLOCK "StringFileInfo"
 {
  BLOCK "040904E4"
  {
   VALUE "CompanyName",      "Simutrans-Experimental Team"
   VALUE "FileDescription",  "Simutrans-Experimental"
   VALUE "FileVersion",      VERSION_NUMBER EXPERIMENTAL_VERSION
   VALUE "InternalName",     "SIMUTRANS"
   VALUE "LegalCopyright",   "\251 Hj. Malthaner '97-'04 Simutrans-Experimental Team '05-'13"
   VALUE "OriginalFilename", "simutrans-experimental.exe"
   VALUE "ProductName",      "Simutrans-Experimental"
   VALUE "ProductVersion",   VERSION_NUMBER EXPERIMENTAL_VERSION
  }

 }

 BLOCK "VarFileInfo"
 {
  VALUE "Translation", 2057, 1252, 1031, 1252, 1035, 1252, 1036, 1252, 1034, 1252, 1038, 1252, 1043, 1252
 }

}
